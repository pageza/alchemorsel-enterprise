{
  "dashboard": {
    "id": null,
    "title": "Alchemorsel Business Metrics",
    "tags": ["alchemorsel", "business", "kpi", "product"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Key Business Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(recipes_created_total[1h])",
            "legendFormat": "Recipes Created (1h)",
            "refId": "A"
          },
          {
            "expr": "increase(users_registered_total[1h])",
            "legendFormat": "New Users (1h)",
            "refId": "B"
          },
          {
            "expr": "increase(recipes_viewed_total[1h])",
            "legendFormat": "Recipe Views (1h)",
            "refId": "C"
          },
          {
            "expr": "increase(ai_requests_total[1h])",
            "legendFormat": "AI Requests (1h)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Recipe Creation Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(recipes_created_total[5m]) * 3600",
            "legendFormat": "Recipes/hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "User Registration Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(users_registered_total[5m]) * 3600",
            "legendFormat": "Users/hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 4,
        "title": "AI Service Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ai_requests_total[5m])) by (provider, model)",
            "legendFormat": "{{provider}}/{{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "AI Request Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "slo:ai_request_success_rate",
            "legendFormat": "Success Rate",
            "refId": "A"
          },
          {
            "expr": "95",
            "legendFormat": "Target (95%)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "max": 100,
            "min": 90
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 6,
        "title": "Recipe View to Creation Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "(increase(recipes_created_total[1h]) / increase(recipes_viewed_total[1h])) * 100",
            "legendFormat": "Conversion Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 10}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 7,
        "title": "User Engagement by Feature",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total{path=~\".*/recipes.*\"}[1h]))",
            "legendFormat": "Recipe Management",
            "refId": "A"
          },
          {
            "expr": "sum(increase(http_requests_total{path=~\".*/search.*\"}[1h]))",
            "legendFormat": "Search",
            "refId": "B"
          },
          {
            "expr": "sum(increase(http_requests_total{path=~\".*/ai.*\"}[1h]))",
            "legendFormat": "AI Features",
            "refId": "C"
          },
          {
            "expr": "sum(increase(http_requests_total{path=~\".*/profile.*\"}[1h]))",
            "legendFormat": "Profile Management",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 8,
        "title": "Business Success Rates",
        "type": "timeseries",
        "targets": [
          {
            "expr": "slo:recipe_creation_success_rate",
            "legendFormat": "Recipe Creation",
            "refId": "A"
          },
          {
            "expr": "slo:user_registration_success_rate",
            "legendFormat": "User Registration",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent",
            "max": 100,
            "min": 90
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 9,
        "title": "Daily Active Users (Estimate)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "count by ()(group by (remote_addr)(http_requests_total{service=\"alchemorsel-api\"}))",
            "legendFormat": "Unique IPs",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      },
      {
        "id": 10,
        "title": "AI Cost Tracking",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(ai_request_cost_total[1h])",
            "legendFormat": "Hourly AI Cost",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
      },
      {
        "id": 11,
        "title": "Feature Adoption Rate",
        "type": "bargauge",
        "targets": [
          {
            "expr": "(increase(ai_requests_total[24h]) / increase(users_registered_total[24h])) * 100",
            "legendFormat": "AI Feature Adoption",
            "refId": "A"
          },
          {
            "expr": "(increase(recipes_created_total[24h]) / increase(users_registered_total[24h])) * 100",
            "legendFormat": "Recipe Creation Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "time_range",
          "type": "interval",
          "query": "1h,6h,12h,24h,7d,30d",
          "current": {
            "value": "24h",
            "text": "24h"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "increase(deployment_events_total[1m])",
          "titleFormat": "Deployment",
          "textFormat": "New version deployed"
        }
      ]
    },
    "schemaVersion": 37,
    "version": 1
  }
}